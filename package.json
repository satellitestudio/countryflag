{
  "name": "countryflag",
  "description": "return an emoji flag or possible fallbacks for a given iso2 code",
  "version": "3.1.2",
  "main": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "license": "MIT",
  "scripts": {
    "generate-countries": "ts-node -P ./scripts/tsconfig.tsnode.json ./scripts/getCountries.ts",
    "clean": "rimraf dist/*",
    "build": "cross-env NODE_ENV=production rollup --config rollup.config.js",
    "prepublish": "yarn generate-countries && yarn clean && yarn build",
    "dev": "rollup --config rollup.config.js --watch"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^4.1.1",
    "@satellitestudio/eslint-config": "1.2.1",
    "rimraf": "^3.0.2",
    "rollup": "2.10.4",
    "rollup-plugin-terser": "^5.3.0",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "core-js": "3.1.4",
    "cross-env": "^7.0.2",
    "if-emoji": "0.1.0",
    "world-countries": "4.0.0"
  }
}
